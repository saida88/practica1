from django.forms import ModelForm
from models import *

class EstilForm(ModelForm):
	class Meta:
		model = Estil
		exclude = ('idEstil')

class ModelMaterial(ModelForm):
	class Meta:
		model = Material
		exclude = ('idMaterial')

class ArquitecteForm(ModelForm):
	class Meta:
		model = Arquitecte
		exclude = ('idArquitecte')

class GratacelForm(ModelForm):
	class Meta:
		model = Gratacel
		exclude = ('idGratacel')

